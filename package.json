    "@pushrocks/smartshell": "^2.0.23",
    "@pushrocks/smartsocket": "^1.1.45",
    "@pushrocks/smartssh": "^1.2.3",
    "@pushrocks/smartstring": "^3.0.10",
    "@types/shelljs": "^0.8.5",
    "@types/through2": "^2.0.34",
    "through2": "^3.0.1"
